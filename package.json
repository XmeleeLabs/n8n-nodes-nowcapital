{
	"name": "@nowcapital/n8n-nodes-nowcapital",
	"publishConfig": {
		"access": "public"
	},
	"version": "1.0.6",
	"description": "n8n node for NowCapital.ca Canadian Retirement Planning API",
	"license": "MIT",
	"homepage": "https://nowcapital.ca",
	"keywords": [
		"n8n-community-node-package",
		"retirement",
		"canada",
		"financial-planning",
		"nowcapital"
	],
	"author": {
		"name": "XmeleeLabs",
		"email": "support@nowcapital.ca"
	},
	"repository": {
